#!/bin/sh

package_name="libtasn1"
package_version="4.20.0"
package_description=""
package_source="https://ftp.gnu.org/gnu/libtasn1/libtasn1-4.20.0.tar.gz 930f71d788cf37505a0327c1b84741be"

source_dir="$package_name-$package_version"

configure() {
    cd "$source_dir" || log_error "In $package_name.build: Failed to cd to $source_dir"
	./configure --prefix=/usr --disable-static
}

build() {
    make
}

install_files() {
    make install
}
