#!/bin/sh

package_name="psmisc"
package_version="23.7"
package_description="Programs for displaying information about running processes"
package_source="https://sourceforge.net/projects/psmisc/files/psmisc/psmisc-23.7.tar.xz"

source_dir="$package_name-$package_version"

configure() {
    ./configure --prefix=/usr
}

build() {
    make
}

install_files() {
    make DESTDIR="$destdir" install
}
